@model EntityLayer.Concrete.Content
@{
    ViewBag.Title = "AddContent";
    Layout = "~/Views/Shared/_WriterLayout.cshtml";
}


<br />
<h2>Yeni İçerik</h2>
<br />
<form method="post" class="form-group">

    <label>İçerik</label>
    @Html.TextAreaFor(p => p.ContentValue, 10, 30, new { @class = "form-control", required = "" })
    <br />

    <button class="btn btn-primary">Oluştur</button>

</form>